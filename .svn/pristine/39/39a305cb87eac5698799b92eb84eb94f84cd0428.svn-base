  import footerSame from '../component/footerSame'
  import bottomTap from '../component/bottomTap'
    export default {
        name: "Home",
      components:{
        footerSame,
        bottomTap
      },
      data(){
          return {
            showing: true,
            time: 60,
            verifyCode: '发送验证码',
            borrowerData:{
              borrowerName:'',
              borrowerSex:1,
              loanAmount:'',
              address1:'',
              address2:'',
              phone:'',
              code:'',
            },
            //省
            provinceData:[
              {
                pid:'',
                provincial:'',
              }
            ],
            //市 区
            cityData:[
              {
                cid:'',
                city:'',
                pid:'',
              }
            ],
            specialData:[
              {
                productId:'',
                productType:'',
                productPublisher:'',
                productInterest:'',
                productStartAmount:'',
                productEndAmount:'',
                productName:'',
                producLoanLength:'',
                productLife:'',
              },
            ],
            productData:[
              {
                productType:'',
                productPublisher:'',
                productInterest:'',
                productStartAmount:'',
                productEndAmount:'',
                productName:'',
                producLoanLength:'',
                productLife:'',
                productStartAmount:''
              },
            ],
            agentData:[
              {
                userBroker:  {
                  brokerId:'',
                  brokerName:'',
                  image:'',
                  job:'',
                }
              }
            ],
            organData:[
              {
                agencyId:'',
                agencyName:'',
                agencyIntroduction:'',
                agencyLogo:''
              }
            ],
            loanData:[]
          }
      },
      methods:{
        apply(){
             if(this.borrowerData.borrowerName && this.borrowerData.loanAmount && this.borrowerData.address1 && this.borrowerData.address2 && this.borrowerData.phone && this.borrowerData.code){
                  let data =new FormData()
                  for(let item in this.borrowerData){
                    data.append(item,this.borrowerData[item])
                  }
                  this.$axios.post(`orderPublic/createPublicOrder`,data).then(res=>{
                    console.log('sss',res);
                    if(res.status === 200){
                      this.$message.success("申请成功")
                      this.borrowerData={}
                      this.$router.push('/applyVictory')
                    }else{
                      this.$message.warning(res.msg)
                    }
                  })
             }else{
               this.$message.warning("不能为空")
             }
           },
        //发送验证码
        send() {
          if(this.borrowerData.phone){
            this.showing=false;
            this.$axios.get(`base/getUpdatePhoneCode/${this.borrowerData.phone}`).then(res=>{
              let timer=setInterval(()=>{
                this.time--
                if(this.time < 0){
                  clearInterval(timer)
                  this.showing=true
                  this.verifyCode='重新获取'
                  this.time=20
                }
              },1000)
            })
          }else{
            this.$message.warning('手机号不能为空')
          }

        },
        //获取省
        getProvince(){
          this.$axios.get('city/getAllProvincial').then(res=>{
            this.provinceData=res
          })
        },
        //获取 市 区
        getCity(val){
          this.$axios.get(`city/getAllCity/${val}`).then(res=>{
            this.cityData=res
          })
        },
          //特别推荐详情页
        specialLook(id){
          this.$router.push(`/productDetail/${id}`)
        },
        proDetail(id){
          this.$router.push(`/productDetail/${id}`)
        },
        orDetail(id){
          this.$router.push(`/organDetail/${id}`)
        },
        agDetail(id){
          this.$router.push(`/agentDetail/${id}`)
        },


          //特别推荐
        getSpecial(){
          this.$axios.get('product/getHotPro/1/4').then(res=>{
            this.specialData=res
          })
        },
        //产品推荐
        getProduct(){
          this.$axios.get('product/getLimitPro/1/3').then(res=>{
            this.productData=res.list
          })
        },
        //经纪人推荐
        getAgentData(){
          this.$axios.get('userBroker/showPageUserBroker').then(res=>{
            this.agentData=res.rows
          })
        },
        //机构推荐
        getOrganData(){
          this.$axios.get(`userAgency/getLimitPage/1/8`).then(res=>{
                this.organData=res.list
            console.log('dd',res.list);
          })
        },
        getLoanData(){
          this.$axios.get('userBorrower/indexBorrower').then(res=>{
            this.loanData = res.map(item =>{
              let fristName = item.borrowerName === null ? '无' : item.borrowerName.substr(0,1)
              let allName = item.borrowerSex === 0 ? `${fristName}女士` : `${fristName}先生`
              let phoneG = item.phone.substr(0,3)+"*****"+item.phone.substr(8,10)
              return {...item, allName,phoneG}
            })
            console.log('aaa',this.loanData)
          })
        }
      },
      created(){
          this.getSpecial()
          this.getProduct()
          this.getAgentData()
          this.getOrganData()
          this.getLoanData()
          this.getProvince()
      }
    }
</script>

